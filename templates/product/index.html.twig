{% extends 'base.html.twig' %}

{% block title %}<title>Produit</title>{% endblock %}

{% block body %}
    <h1 class="productTitle">Tous les nounours</h1>
    <div class="container">
        <div class="row">
            <div class="col-md-5">

                {% include 'product/filter.html.twig' with {form: form} only%}

            </div>
        </div>
        <div class="col-md-11">
            <div class="grid">
                {% for product in products %}
                    {% include '/allproduct/card.html.twig' with {product: product} only%}
                {% endfor %}
            </div>
            <div>
                {{ knp_pagination_render(products) }}
            </div>

        </div>




    </div>
{% endblock %}
